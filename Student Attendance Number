#include <stdio.h>
int main()
{
    int i;
    int totalStudents;
    printf("Student Attendance Number Program\n");
    printf("Enter total number of students: ");
    scanf("%d", &totalStudents);
    if(totalStudents <= 0)
    {
        printf("Invalid number\n");
        return 0;
    }
    printf("\nAttendance Numbers:\n");
    for(i = 1; i <= totalStudents; i++)
    {
        printf("%d\n", i);
    }
    return 0;
}
