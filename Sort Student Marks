#include <stdio.h>

int main() {
    int marks[5], i, j, temp;

    printf("Sort Student Marks\n\n");

    printf("Enter marks of 5 students:\n");
    for(i=0;i<5;i++)
        scanf("%d",&marks[i]);

    printf("Marks before sorting: ");
    for(i=0;i<5;i++)
        printf("%d ",marks[i]);
    printf("\n");

    for(i=0;i<4;i++) {
        for(j=i+1;j<5;j++) {
            if(marks[i]>marks[j]) {
                temp=marks[i];
                marks[i]=marks[j];
                marks[j]=temp;
            }
        }
    }

    printf("Marks after sorting: ");
    for(i=0;i<5;i++)
        printf("%d ",marks[i]);
    printf("\n");

    return 0;
}
