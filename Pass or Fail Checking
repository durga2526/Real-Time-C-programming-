#include <stdio.h>
int main()
{
    float marks;
    printf("Pass or Fail Checking Program\n");
    printf("Enter the student marks: ");
    scanf("%f", &marks);
    if(marks < 0 || marks > 100)
    {
        printf("Invalid marks\n");
        return 0;
    }
    if(marks >= 50)
    {
        printf("\nResult: PASS\n");
    }
    else
    {
        printf("\nResult: FAIL\n");
    }
    return 0;
}
