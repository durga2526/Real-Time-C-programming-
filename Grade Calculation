#include <stdio.h>
int main()
{
    float marks;
    printf("Grade Calculation Program\n");
    printf("Enter marks: ");
    scanf("%f", &marks);
    if(marks < 0 || marks > 100)
    {
        printf("Invalid marks\n");
        return 0;
    }
    if(marks >= 90)
    {
        printf("\nGrade: A\n");
    }
    else if(marks >= 75)
    {
        printf("\nGrade: B\n");
    }
    else if(marks >= 60)
    {
        printf("\nGrade: C\n");
    }
    else if(marks >= 50)
    {
        printf("\nGrade: D\n");
    }
    else
    {
        printf("\nResult: FAIL\n");
    }
    return 0;
}
