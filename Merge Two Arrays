#include <stdio.h>
int main() {
    int a[5], b[5], merged[10];
    int i, j=0;

    printf("Merge Two Arrays\n\n");

    printf("Enter 5 elements of first array:\n");
    for(i=0;i<5;i++) {
        scanf("%d",&a[i]);
        merged[j++] = a[i];
    }

    printf("Enter 5 elements of second array:\n");
    for(i=0;i<5;i++) {
        scanf("%d",&b[i]);
        merged[j++] = b[i];
    }

    printf("\nMerged array: ");
    for(i=0;i<10;i++)
        printf("%d ",merged[i]);
    printf("\n");

    return 0;
}
