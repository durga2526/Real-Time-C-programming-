#include <stdio.h>

int main()
{
    float temp[7];
    float highest;
    float lowest;
    int i;

    printf("Weekly Temperature Analysis Program\n");

    for(i = 0; i < 7; i++)
    {
        printf("Enter temperature for day %d: ", i + 1);
        scanf("%f", &temp[i]);
    }

    highest = temp[0];
    lowest = temp[0];

    for(i = 1; i < 7; i++)
    {
        if(temp[i] > highest)
        {
            highest = temp[i];
        }
        if(temp[i] < lowest)
        {
            lowest = temp[i];
        }
    }

    printf("\n");
    printf("Highest Temperature: %.2f\n", highest);
    printf("Lowest Temperature : %.2f\n", lowest);

    return 0;
}
