#include <stdio.h>

int main() {
    int a[50], b[50];
    int n, i;

    printf("Copy Array Elements\n\n");

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter elements of first array:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    for(i = 0; i < n; i++) {
        b[i] = a[i];
    }

    printf("\nFirst Array:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }

    printf("\nSecond Array:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", b[i]);
    }

    return 0;
}
