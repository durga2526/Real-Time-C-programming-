#include <stdio.h>
int main()
{
    int n, i;
    long long fact = 1;

    printf("Factorial Program\n");
    printf("Enter a number: ");
    scanf("%d", &n);
    if(n < 0)
    {
        printf("Invalid input\n");
        return 0;
    }

    for(i = 1; i <= n; i++)
    {
        fact = fact * i;
    }
    printf("Factorial: %lld\n", fact);
    return 0;
}
