#include <stdio.h>
#include <string.h>

struct Mobile {
    char brand[50];
    char model[50];
    float price;
};

int main() {
    struct Mobile m;

    printf("Store Mobile Details\n\n");

    printf("Enter mobile brand: ");
    fgets(m.brand, sizeof(m.brand), stdin);
    m.brand[strcspn(m.brand, "\n")] = '\0';

    printf("Enter mobile model: ");
    fgets(m.model, sizeof(m.model), stdin);
    m.model[strcspn(m.model, "\n")] = '\0';

    printf("Enter mobile price: ");
    scanf("%f", &m.price);

    printf("\nMobile Details:\n");
    printf("Brand: %s\n", m.brand);
    printf("Model: %s\n", m.model);
    printf("Price: %.2f\n", m.price);

    return 0;
}
