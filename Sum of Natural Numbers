#include <stdio.h>

int main()
{
    int n;
    int i;
    int sum = 0;

    printf("Sum of Natural Numbers Program\n");

    printf("Enter value of N: ");
    scanf("%d", &n);

    if(n <= 0)
    {
        printf("Invalid input\n");
        return 0;
    }

    for(i = 1; i <= n; i++)
    {
        sum = sum + i;
    }

    printf("\n");
    printf("Sum: %d\n", sum);

    return 0;
}
